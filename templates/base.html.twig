<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}
        <div class="page">
            <header><h2>Header</h2></header>
            <aside>
                <h2>Side Bar</h2>
            </aside>
            <section class="products">
                <h2>Products</h2>
            </section>
            <div class="feature2">
                <h2>Feature2</h2>
            </div>
            <div class="feature3">
                <h2>Feature3</h2>
            </div>
            <div class="bonus1">
                <h2>Bonus1</h2>
            </div>
            <div class="bonus2">
                <h2>Bonus2</h2>
            </div>
            <div class="bonus3">
                <h2>Bonus3</h2>
            </div>
            <div class="bonus4">
                <h2>Bonus4</h2>
            </div>
            <footer>
                <h2>Footer</h2>
            </footer>
        </div>
        {% endblock %}
    </body>
</html>
