<!DOCTYPE html>
<html lang="en-us">
    <head>
        <title>{% block pageTitle %}{{ pageTitle|default('Selene Commerce') }}{% endblock %}</title>
        <meta charset="UTF-8">
        <meta name="description" content=" Divisima | eCommerce Template">
        <meta name="keywords" content="divisima, eCommerce, creative, html">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Favicon -->
        <link href="/build/img/favicon.ico" rel="shortcut icon"/>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

    </head>
    <body>
        <!-- Page Preloder -->
        {% block preloader %}
            <div id="preloder">
                <div class="loader"></div>
            </div>
        {% endblock %}

        <!-- Header section -->
        {% block header %}
            {{ include('includes/header.html.twig') }}
        {% endblock %}
        <!-- Header section end -->

        <!-- Hero section -->
        {# this section is added as it is needed on each page #}
        {% block hero %}
        {% endblock %}
        <!-- Hero section end -->

        <!-- Features section -->
        {# this section is added as it is needed on each page #}
        {% block features %}
        {% endblock %}
        <!-- Features section end -->

        <!--Page Info section -->
        {% block pageinfo %}
        {% endblock %}

        <!-- Categories section -->
        {% block categories %}
        {% endblock %}

        <!-- Products section -->
        {% block product %}
        {% endblock %}

        <!-- letest product section -->
        {# this section is added as it is needed on each page #}
        {% block latest %}
        {% endblock %}
        <!-- letest product section end -->

        <!-- Product filter section -->
        {# this section is added as it is needed on each page #}
        {% block productfilter %}
        {% endblock %}
        <!-- Product filter section end -->

        <!-- Cart section -->
        {% block cart %}
        {% endblock %}

        <!-- checkout section -->
        {% block checkout %}
        {% endblock %}

        <!-- Related Products section -->
        {% block relatedproducts %}
        {% endblock %}

        <!-- Banner section -->
        {# this section is added as it is needed on each page #}
        {% block banner %}
        {% endblock %}
        <!-- Banner section end  -->

        <!-- Core Body section -->
        {# this section is added as a page needs it, and can be considered a catch-all for generic pages #}
        {% block body %}
        {% endblock %}


        <!-- Footer section -->
        {% block footer %}
            {{ include('includes/footer.html.twig') }}
        {% endblock %}
        <!-- Footer section end -->

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {#            {{ include('includes/trackers.html.twig') }} #}
        {% endblock %}

        {#
        <!--====== Javascripts & Jquery ======-->
        <script src="js/jquery-3.2.1.min.js"></script>
        <script src="js/bootstrap.min.js"></script>
        <script src="js/jquery.slicknav.min.js"></script>
        <script src="js/owl.carousel.min.js"></script>
        <script src="js/jquery.nicescroll.min.js"></script>
        <script src="js/jquery.zoom.min.js"></script>
        <script src="js/jquery-ui.min.js"></script>
        <script src="js/main.js"></script>
        #}
    </body>
</html>
